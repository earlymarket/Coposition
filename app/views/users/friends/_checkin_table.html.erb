<table id="checkin-table" class="highlight">
  <thead>
    <tr>
      <th>Created</th>
      <th>Area</th>
      <th>Fogged</th>
      <th>More Info</th>
    </tr>
  </thead>
  <% @checkins.each do |checkin| %>
    <%= render "checkin_table_row", checkin: checkin %>
  <% end %>
</table>
<%= will_paginate @checkins %>
